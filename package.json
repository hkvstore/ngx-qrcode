{
	"name": "@hkvstore/ngx-qrcode",
	"version": "9.0.5",
	"scripts": {
		"ng": "ng",
		"start": "ng serve ngx-qrcode-demo",
		"build": "ng build --prod",
		"build:demo": "ng build ngx-qrcode-demo --prod",
		"test": "ng test --watch=false",
		"test:watch": "ng test --watch=true",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"copy:readme": "copyfiles README.md ./dist/ngx-qrcode",
		"github-releaser": "conventional-github-releaser -p angular -r 0",
